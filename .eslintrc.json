{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "quotes": [
            "error",
            "double"
        ],
        "no-underscore-dangle":"off",
        "indent": "off",
        "max-len": [
            "warn",
            { "code": 80, "TabWidth": 5 }
        ]
    }
}
